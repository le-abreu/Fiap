<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>handlerFuncionario</managed-bean-name>
		<managed-bean-class>br.com.fiap.handler.HandlerFuncionario</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>handlerLogin</managed-bean-name>
		<managed-bean-class>br.com.fiap.handler.HandlerLogin</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- ADMINISTRADOR -->
	<navigation-rule>
		<display-name>administradorFuncionario</display-name>
		<from-view-id>/ADMINISTRADOR/formFuncionario.jsp</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/ADMINISTRADOR/formFuncionario.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>


	<navigation-rule>
		<from-view-id>/uploadArquivo.jsp</from-view-id>
		<navigation-case>
			<from-outcome>funcionario</from-outcome>
			<to-view-id>/formFuncionario.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/uploadArquivo.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
